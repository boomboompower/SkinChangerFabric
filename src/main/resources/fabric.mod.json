    ],
    "cotton-client-commands": [
      "wtf.boomy.skinchanger.client.commands.SkinChangerClientCommand"
    ]
  },
  "mixins": [
    "skinchanger.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.9.2+build.206",
    "fabric": "*",
    "minecraft": "1.16.4",
    "cotton-client-commands": "^1.0.0"
