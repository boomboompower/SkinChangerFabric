  "id": "skinchangerfabric",
  "version": "${version}",
  "name": "SkinChangerFabric",
  "description": "A lightweight version of the SkinChanger mod, built for Fabric",
  "authors": [
    "boomboompower"
  ],
  "contact": {
    "website": "boomy.wtf"
  },
  "license": "LICENSE.md",
  "environment": "*",
  "entrypoints": {
    "client": [
      "wtf.boomy.skinchanger.client.SkinChangerClient"
